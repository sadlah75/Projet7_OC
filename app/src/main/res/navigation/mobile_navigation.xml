    <fragment
        android:id="@+id/nav_map_view"
        android:name="com.sadek.go4lunch.ui.map.MapViewFragment"
        android:label="@string/toolbar_title"
        tools:layout="@layout/fragment_map_view" />

    <fragment
        android:id="@+id/nav_list_view"
        android:name="com.sadek.go4lunch.ui.list.RestaurantFragment"
        android:label="@string/toolbar_title"
        tools:layout="@layout/fragment_list_view" />

    <fragment
        android:id="@+id/nav_workmates"
        android:name="com.sadek.go4lunch.ui.workmate.WorkmateFragment"
